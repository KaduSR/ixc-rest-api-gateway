{
  "name": "ixc-sdk",
  "version": "0.1.5",
  "description": "SDK para integração com a API do ERP IXC, facilitando o acesso e manipulação de dados através de endpoints REST",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start:api": "ts-node-dev api/index.ts",
    "prepare": "npm run build",
    "build": "microbundle --tsconfig tsconfig.json --no-sourcemap",
    "docs": "typedoc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "SDK",
    "ERP",
    "IXC",
    "typescript"
  ],
  "author": "LSaints",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.17.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^2.5.14",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.10.4",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "microbundle": "^0.15.1",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typedoc": "^0.28.0",
    "typedoc-plugin-markdown": "^4.4.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LSaints/ixc-sdk.git"
  },
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/LSaints/ixc-sdk/issues"
